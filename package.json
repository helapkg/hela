    "test": "standard && node test.js"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "formidable": "^1.0.17",
    "koa-body-parsers": "git+https://github.com/tunnckoCore/body-parsers.git",
    "lazy-cache": "^2.0.0"
  },
  "devDependencies": {
    "is-buffer": "^1.1.3",
    "koa": "^1.2.0",
    "mukla": "^0.4.1",
